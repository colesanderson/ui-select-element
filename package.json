{
    "name": "ui-select-element",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "classnames": "^2.2.6",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-scripts": "3.3.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "build-sprites": "npm run clean:svgs && svg-sprite --symbol --symbol-dest='.' --symbol-sprite='sprite.svg' --shape-id-separator='' --dest=./src/static/svg/sprite-dist ./src/static/svg/sprite-source/*.svg",
        "clean:svgs": "svgo -f ./src/static//svg/sprite-source"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "svg-sprite": "^1.5.0",
        "svgo": "^1.3.2"
    }
}
